<h4 class='section-header'>Editing Citation
  <Control::MdStatus @model={{compute (route-action 'parentModel')}} />
</h4>
{{#with this.model as |citation|}}
  <div class=''>
    <Object::MdCitation
      @model={{citation}}
      @profilePath='record.lineage.citation'
      @editIdentifier={{route-action 'editIdentifier'}}
    />
    {{outlet}}
  </div>
{{/with}}
<ToElsewhere
  @named='md-subbar-extra'
  @send={{component
    'control/subbar-link'
    text='Back to Lineage Object'
    icon='arrow-left'
    click=(route-action 'goBack')
  }}
/>
<ToElsewhere
  @named='md-scroll-spy-record-edit'
  @send={{component
    'control/md-scroll-spy'
    scrollInit=this.scrollTo
    setScrollTo=(route-action 'setScrollTo')
  }}
/>