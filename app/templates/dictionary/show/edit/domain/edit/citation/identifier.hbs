<h4 class='section-header'>
  Editing Identifier:
  {{this.model.identifier}}
  <Control::MdStatus @model={{this.parentModel}} />
</h4>

<div class=''>
  <Object::MdIdentifier
    @class=''
    @model={{this.model}}
    @profilePath='dictionary.domain.citation.identifier'
    @data-spy='Identifier'
  />
  {{outlet}}
</div>

<ToElsewhere
  @named='md-subbar-extra'
  @send={{component
    'control/subbar-link'
    text='Back to Domain Citation'
    icon='arrow-left'
    route='dictionary.show.edit.domain.edit.citation'
  }}
/>
<ToElsewhere
  @named='md-scroll-spy-dict-edit'
  @send={{component
    'control/md-scroll-spy'
    scrollInit=this.scrollTo
    setScrollTo=(route-action 'setScrollTo')
  }}
/>