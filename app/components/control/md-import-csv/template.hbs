{{#if this.isProcessing}}
  <h3>
    <FlightIcon @name='loading' />
    Processing...
    <button class='btn btn-info' type='button' {{action 'stopParsing'}}>
      <FlightIcon @name='stop-circle' />
      Stop</button>
  </h3>
  <div class='progress'>
    <div
      class='progress-bar'
      role='progressbar'
      {{! template-lint-disable no-inline-styles }}
      aria-valuenow={{this.progress}}
      aria-valuemin='0'
      aria-valuemax='100'
      style={{this.barWidth}}
    >
      <span class='sr-only'>{{this.progress}}% Complete</span>
    </div>
  </div>
{{else}}
  <FilePicker
    @class='md-file-picker md-import-picker'
    @fileLoaded='readData'
    @preview={{false}}
    @accept='.csv,.txt'
    @readAs='readAsText'
  >
    <button type='button' class='btn btn-lg btn-info btn-block'>
      <FlightIcon @name='target' />
      Click or Drop a CSV here.
    </button>
  </FilePicker>
