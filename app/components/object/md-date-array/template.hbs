<Object::MdArrayTable
  @columns='Date,Date Type,Description'
  @required={{this.required}}
  @title='Date'
  @value={{this.value}}
  @plain={{this.plain}}
  @validation={{this.validation}}
  @profilePath={{this.profilePath}}
  @data-spy={{this.data-spy}}
  @templateClass={{this.templateClass}}
  as |date|
>
  <Object::MdDate
    @model={{date.item}}
    @profilePath={{concat this.profilePath '.dateItem'}}
  />
  {{yield}}
</Object::MdArrayTable>