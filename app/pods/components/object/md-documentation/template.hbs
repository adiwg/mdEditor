<div class="form-group">
    {{object/md-resource-type-array
      plain=false
      value=model.resourceType
      required=true
      data-spy="Resource Types"
      profilePath=(concat profilePath ".resourceType")
    }}
</div>
{{#let this.model.citation.firstObject as |citation|}}
  <Object::MdCitation
    @model={{citation}}
    @profilePath={{concat this.profilePath ".citation"}}
    @simpleIdentifier={{true}}
  />
{{/let}}
{{yield}}
