<address>
  {{#each this.item.deliveryPoint as |dp|}}
    {{dp}}<br />
  {{/each}}
  {{this.item.city}},
  {{this.item.administrativeArea}}
  {{this.item.postalCode}}
  {{this.item.country}}<br />
  {{#if this.item.addressType}}
    <strong>{{join ', ' this.item.addressType}}</strong>
  {{/if}}
</address>