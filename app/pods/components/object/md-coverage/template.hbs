{{input/md-input
  label="Name"
  data-spy="Name"
  model=this
  valuePath="name"
  placeholder="The name(title) of the coverage."
  profilePath=(concat profilePath ".coverageName")
  showValidations=true
}}

{{input/md-input
  label="Description"
  data-spy="Description"
  model=this
  valuePath="description"
  placeholder="A description of the coverage"
  profilePath=(concat profilePath ".coverageDescription")
  showValidations=true
}}

{{#object/md-object-table
 items=model.processingLevelCode
 header="Processing Level Code"
 buttonText="Add Processing Level Code"
 ellipsis=true
 profilePath=(concat profilePath ".processingLevelCode")
 attributes="identifier"
}}
  {{object/md-plc model=editing profilePath=(concat profilePath ".processingLevelCode")}}
{{/object/md-object-table}}

{{yield}}