</td>
<td class="property">
  <Input::MdInput @model={{this}} @valuePath="definition" @placeholder="A brief definition of the attribute(column)." @profilePath={{concat profilePath ".attribute.definition"}} @showValidations={{true}} />
</td>
<td class="property text-center">
  <Input::MdBoolean @required={{true}} @value={{model.allowNull}} @profilePath={{concat profilePath ".attribute.allowNull"}} />
</td>
