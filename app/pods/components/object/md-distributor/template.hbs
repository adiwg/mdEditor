<div data-spy='Contacts' id={{concat this.elementId '-contacts'}}>
  {{#if this.model.contact}}
    {{input/md-select-contacts
      label='Contacts'
      placeholder='Select one or more contacts'
      model=this
      path='contacts'
      icon=true
      tooltip=true
      showValidations=true
      class='required'
      profilePath=(concat this.profilePath '.contacts')
    }}
    {{input/md-codelist
      label='Role'
      path='role'
      model=this
      create=true
      tooltip=true
      icon=false
      disabled=this.disabled
      mdCodeName='role'
      showValidations=true
      placeholder='Select or enter a role'
      profilePath=(concat this.profilePath '.role')
    }}
  {{/if}}
</div>

{{!-- {{#with model.transferOption.firstObject as |opt|}}
  {{#layout/md-card
    shadow=false
    muted=false
    title="Transfer Options"
    plain=true
    collapsible=true
    collapsed=false
    bodyIsRow=false
  }}
    {{object/md-transfer model=opt profilePath=(concat profilePath ".transferOption")}}
  {{/layout/md-card}}
{{/with}} --}}

{{object/md-objectroute-table
  attributes='Size(MB),online?,offline?,format?'
  items=this.model.transferOption
  header='Transfer Options'
  shadow=false
  buttonText='Add Transfer Options'
  ellipsis=true
  editItem=this.editTransferOption
  previewTemplateTable='object/md-transfer/preview'
  profilePath=(concat this.profilePath '.transferOption')
  verticalButtons=false
  hideIndex=false
  condensed=false
  editOnAdd=true
  scrollToId=(concat 'md-transfer-' this.distributorId)
  routeParams=this.index
  data-spy='Transfer Options'
}}

{{#with this.model.orderProcess.firstObject as |order|}}
  {{#layout/md-card
    shadow=false
    muted=false
    title='Order Process'
    plain=true
    collapsible=true
    collapsed=false
    bodyIsRow=false
    profilePath=(concat this.profilePath '.orderProcess')
  }}
    {{! <label class="text-info col-sm-2">Order Process</label>
    <div class="col-sm-10"> }}
    {{input/md-textarea
      value=order.fees
      placeholder='Describe the fees and terms for retrieving the resource, including monetary units.'
      label='Fees'
      autoresize=false
      profilePath=(concat this.profilePath '.orderProcess.fees')
      embedded=true
      data-spy='Fees'
    }}
    {{input/md-datetime
      date=order.plannedAvailability
      placeholder='Date when the resource will be avialable'
      label='Planned Availability'
      profilePath=(concat this.profilePath '.orderProcess.plannedAvailability')
    }}
    {{input/md-textarea
      value=order.orderingInstructions
      maxheight=100
      placeholder='Describe general instructions for obtaining the resource'
      label='Ordering Instructions'
      profilePath=(concat this.profilePath '.orderProcess.orderingInstructions')
    }}
    {{input/md-textarea
      value=order.turnaround
      autoresize=false
      placeholder='Describe the typical turnaround time for the filling of an order'
      label='Turnaround'
      profilePath=(concat this.profilePath '.orderProcess.turnaround')
    }}
    {{! </div> }}
  {{/layout/md-card}}
{{/with}}
{{yield}}