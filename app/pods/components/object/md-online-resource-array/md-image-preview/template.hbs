  <div class="md-preview-image text-center">
    {{#if this.item.uri}}
    <img src={{this.item.uri}} class="img-responsive img-thumbnail" alt="Image Preview">
    {{else}}
    URI Not Defined
    {{/if}}
  </div>
<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd>
  <div class="wrap">{{html-safe (if this.item.name this.item.name "<em>Not Defined</em>")}}</div>
  </dd>
  <dt>URI</dt>
  <dd>
    <div class="wrap">{{html-safe (if this.item.uri this.item.uri "<em>Not Defined</em>")}}</div>
  </dd>
</dl>
