{{#object/md-object-table
  items=this.model
  header=this.label
  buttonText="Add Identifier"
  ellipsis=this.ellipsis
  templateClass=this.templateClass
  attributes=this.attributes
 as |editing|
}}

{{object/md-identifier
  model=editing
  short=this.short
  profilePath=this.profilePath
}}

{{!-- {{input/md-input
  value=editing.identifier
  placeholder="Enter the identifier for the resource."
  label="Identifier"
  required=true
  profilePath=(concat this.profilePath 'identifier')
}}

{{input/md-codelist
  label="Namespace"
  placeholder="Select or type a namespace for the identifier."
  create=true
  required=false
  tooltip=true
  icon=false
  disabled=this.disabled
  allowClear=true
  mdCodeName="namespace"
  valuePath="code"
  value=editing.namespace
  profilePath=(concat this.profilePath 'namespace')
}}

{{input/md-input
  value=editing.version
  placeholder="Enter the version for the identifier."
  label="Version"
  profilePath=(concat this.profilePath 'version')
}}

{{input/md-textarea
  value=editing.description
  profilePath=(concat this.profilePath 'description')
  label="Description"
  placeholder="Enter a description of the identifier."
}} --}}
{{/object/md-object-table}}
