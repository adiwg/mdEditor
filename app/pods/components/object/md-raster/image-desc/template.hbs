<Layout::MdCard @collapsible={{true}} @title="Image Quality Code" @collapsed={{false}} @shadow={{false}} @required={{v-get this "isInvalid"}} @profilePath={{concat this.profilePath ".imageQualityCode"}} @class="md-embedded">
  <Object::MdIdentifier @model={{this.model.imageQualityCode}} @profilePath={{concat this.profilePath ".imageQualityCode"}} @collapsed={{true}} @collapsible={{true}} @data-spy={{false}} />
</Layout::MdCard>

<Layout::MdWrap @class="md-flex-col-275">
  <Input::MdInput @value={{this.model.illuminationElevationAngle}} @type="number" @label="Illumination Elevation Angle" @infotip={{true}} @data-spy="Illumination Elevation Angle" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Illumination elevation measured in degrees clockwise from the target plane at intersection of the optical line of sight
    with the Earthâ€™s surface. For images from a scanning device, refer to the centre pixel of the image." @profilePath={{concat this.profilePath ".illuminationElevationAngle"}} />

  <Input::MdInput @value={{this.model.illuminationAzimuthAngle}} @type="number" @label="Illumination Azimuth Angle" @infotip={{true}} @data-spy="Illumination Azimuth Angle" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Illumination azimuth measured in degrees clockwise from true north at the time the image is taken. For images from a
    scanning device, refer to the centre pixel of the image." @profilePath={{concat this.profilePath ".illuminationAzimuthAngle"}} />

  <Input::MdInput @value={{this.model.imagingCondition}} @label="Imaging Condition" @infotip={{true}} @data-spy="Imaging condition" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Code which indicates conditions which may affect the image." @profilePath={{concat this.profilePath ".imagingCondition"}} />

  <Input::MdInput @value={{this.model.cloudCoverPercent}} @type="number" @label="Cloud Cover Percent " @infotip={{true}} @data-spy="Cloud Cover Percent" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Area of the dataset obscured by clouds, expressed as a percentage of the spatial extent." @profilePath={{concat this.profilePath ".cloudCoverPercent"}} />

  <Input::MdInput @value={{this.model.compressionQuantity}} @type="number" @label="Compression Quantity" @infotip={{true}} @data-spy="Compression Quantity" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Count of the number of lossy compression cycles performed on the image." @profilePath={{concat this.profilePath ".compressionQuantity"}} />

  <Input::MdInput @value={{this.model.triangulationIndicator}} @type="boolean" @label="Triangulation Indicator" @infotip={{true}} @data-spy="Triangulation Indicator" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Indication of whether or not triangulation has been performed upon the image." @profilePath={{concat this.profilePath ".triangulationIndicator"}} />

  <Input::MdInput @value={{this.model.radiometricCalibrationAvailable}} @type="boolean" @label="Radiometric Calibration Available" @infotip={{true}} @data-spy="Radiometric Calibration Available" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Indication of whether or not the radiometric calibration information for generating the radiometrically calibrated standard data product is available." @profilePath={{concat this.profilePath ".radiometricCalibrationAvailable"}} />

  <Input::MdInput @value={{this.model.cameraCalibrationAvailable}} @type="boolean" @label="Camera Calibration Available" @infotip={{true}} @data-spy="Camera Calibration Available" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Indication of whether or not constants are available which allow for camera calibration corrections." @profilePath={{concat this.profilePath ".cameraCalibrationAvailable"}} />

  <Input::MdInput @value={{this.model.filmDistortionAvailable}} @type="boolean" @label="Film Distortion Available" @infotip={{true}} @data-spy="Film Distortion Available" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Indication of whether or not Calibration Reseau information is available." @profilePath={{concat this.profilePath ".filmDistortionAvailable"}} />

  <Input::MdInput @value={{this.model.lensDistortionAvailable}} @type="boolean" @label="Lens Distortion Available" @infotip={{true}} @data-spy="Lens Distortion Available" @class="md-embedded col-lg-6 col-xxxl-4" @placeholder="Indication of whether or not lens aberration correction information is available." @profilePath={{concat this.profilePath ".lensDistortionAvailable"}} />
</Layout::MdWrap>

