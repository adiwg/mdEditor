{{!-- {{#if link.isOverflow}} --}}
  {{#if isStaticLinks}}
    <LinkTo @route={{link.target}} class="link.class">{{link.title}}
      {{#if link.tip}}
        <EmberTooltip @effect="slide" @side="bottom" @delay={{500}} @tooltipClass="ember-tooltip md-tooltip info">
      {{link.tip}}
        </EmberTooltip>
      {{/if}}
    </LinkTo>
  {{else}}
    <LinkTo @route={{link.target}} @model={{model}} class="link.class">{{link.title}}
      {{#if link.tip}}
        <EmberTooltip @effect="slide" @side="bottom" @delay={{500}} @tooltipClass="ember-tooltip md-tooltip info">
          {{link.tip}}
          {{!-- {{link.isOverflow}} --}}
        </EmberTooltip>
      {{/if}}
    </LinkTo>
  {{/if}}
{{!-- {{/if}} --}}
