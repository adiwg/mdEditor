{{!-- {{#if link.isOverflow}} --}}
  {{#if isStaticLinks}}
    {{#link-to link.target class="link.class" }}{{link.title}}
      {{#if link.tip}}
        {{#ember-tooltip
          effect="slide"
          side="bottom"
          delay=500
          tooltipClass="ember-tooltip md-tooltip info"
        }}
      {{link.tip}}
        {{/ember-tooltip}}
      {{/if}}
    {{/link-to}}
  {{else}}
    {{#link-to link.target model class="link.class" }}{{link.title}}
      {{#if link.tip}}
        {{#ember-tooltip
          effect="slide"
          side="bottom"
          delay=500
          tooltipClass="ember-tooltip md-tooltip info"
        }}
          {{link.tip}}
          {{!-- {{link.isOverflow}} --}}
        {{/ember-tooltip}}
      {{/if}}
    {{/link-to}}
  {{/if}}
{{!-- {{/if}} --}}
