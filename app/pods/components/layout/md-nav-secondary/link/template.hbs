{{!-- {{#if link.isOverflow}} --}}
  {{#if this.isStaticLinks}}
    <LinkTo @route={{this.link.target}} class="link.class">{{this.link.title}}
      {{#if this.link.tip}}
        {{#ember-tooltip
          effect="slide"
          side="bottom"
          delay=500
          tooltipClass="ember-tooltip md-tooltip info"
        }}
      {{this.link.tip}}
        {{/ember-tooltip}}
      {{/if}}
    </LinkTo>
  {{else}}
    <LinkTo @route={{this.link.target}} @model={{this.model}} class="link.class">{{this.link.title}}
      {{#if this.link.tip}}
        {{#ember-tooltip
          effect="slide"
          side="bottom"
          delay=500
          tooltipClass="ember-tooltip md-tooltip info"
        }}
          {{this.link.tip}}
          {{!-- {{link.isOverflow}} --}}
        {{/ember-tooltip}}
      {{/if}}
    </LinkTo>
  {{/if}}
{{!-- {{/if}} --}}
