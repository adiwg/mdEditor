{{!-- {{#if link.isOverflow}} --}}
{{#if this.isStaticLinks}}
  {{#link-to this.link.target class='link.class'}}{{this.link.title}}
    {{#if this.link.tip}}
      {{#ember-tooltip
        effect='slide'
        side='bottom'
        delay=500
        tooltipClass='ember-tooltip md-tooltip info'
      }}
        {{this.link.tip}}
      {{/ember-tooltip}}
    {{/if}}
  {{/link-to}}
{{else}}
  {{#link-to this.link.target this.model class='link.class'}}{{this.link.title}}
    {{#if this.link.tip}}
      {{#ember-tooltip
        effect='slide'
        side='bottom'
        delay=500
        tooltipClass='ember-tooltip md-tooltip info'
      }}
        {{this.link.tip}}
        {{!-- {{link.isOverflow}} --}}
      {{/ember-tooltip}}
    {{/if}}
  {{/link-to}}
{{/if}}
{{!-- {{/if}} --}}