{{#if this.modal}}
  <ModalDialog @onClose={{action "closeModal"}} @translucentOverlay={{true}} @containerClass="md-jsmodal-container" @overlayClass="md-modal-overlay">
    <button type="button" {{action "closeModal"}} class="md-modal-close" aria-label="Close">
      <span class="fa-stack">
          <FaIcon @icon="circle" @size="2x" />
          <FaIcon @icon="times" class="fa-inverse" />
      </span>
    </button>
    <div class="md-viewer-body"></div>
  </ModalDialog>
{{else}}
  <div class="well">
    <div class="btn-toolbar md-viewer-controls pull-right">
      <div class="btn-group-vertical" role="group" aria-label="JSON viewer controls">
        <button type="button" class="btn btn-primary" {{action "expand"}} title="Expand All"><FaIcon @icon="plus" /></button>
        <button type="button" class="btn btn-default" {{action "collapse"}} title="Collapse All"><FaIcon @icon="minus" /></button>
      </div>
      <div class="btn-group-vertical" role="group" aria-label="JSON viewer controls">
        <button type="button" class="btn btn-primary" {{action "zoomin"}} title="Zoom in"><FaIcon @icon="magnifying-glass-plus" /></button>
        <button type="button" class="btn btn-default" {{action "zoomout"}} title="Zoom Out"><FaIcon @icon="magnifying-glass-minus" /></button>
      </div>
    </div>
    <div class="md-viewer-body">
    </div>
  </div>
{{/if}}
