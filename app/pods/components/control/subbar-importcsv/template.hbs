<button
  type='button'
  class='btn btn-lg btn-info btn-block md-btn-responsive'
  {{action 'doImport' target=this.actionContext}}
>
  <FlightIcon @name='sign-in' />
  Do Import</button>
<button
  type='button'
  class='btn btn-lg btn-danger btn-block md-btn-responsive'
  {{action 'cancelImport' target=this.actionContext}}
>
  <FlightIcon @name='x' />
  Cancel Import</button>
{{yield}}