{{#if (has-block)}}
  {{yield}}
{{else}}
  {{#if this.icon}}
    {{fa-icon this.icon spin=this.iconSpin}}
  {{/if}}
  {{this.text}}
{{/if}}

{{#if this.tooltip}}
  {{ember-tooltip
    tooltipClass=(concat 'ember-tooltip md-tooltip ' this.tipClass)
    text=this.tooltip
    side=this.tipSide
  }}
{{/if}}