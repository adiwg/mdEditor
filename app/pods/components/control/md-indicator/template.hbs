{{fa-icon icon fixedWidth=true}}

{{#ember-popover
  popoverHideDelay=popoverHideDelay
  popperContainer=popperContainer
  tooltipClass=(concat "md-tooltip " type)
  event=event
}}
  {{#if title}}
    <h4 class="title">
      {{#if icon}}
        {{fa-icon icon}}
      {{/if}}
      {{title}}
    </h4>
  {{/if}}
  <span>{{interpolated}}</span>
  {{yield}}
{{/ember-popover}}
