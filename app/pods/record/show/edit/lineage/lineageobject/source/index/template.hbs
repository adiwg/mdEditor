<h4 class='section-header'>Editing Source #{{this.sourceId}}
  <Control::MdStatus @model={{compute (route-action 'parentModel')}} />
</h4>
{{#with this.model as |source|}}
  <div class=''>
    <Object::MdSource @model={{source}} @profilePath='record.lineage.source' />
    {{outlet}}
  </div>
{{/with}}
<ToElsewhere
  @named='md-subbar-extra'
  @send={{component
    'control/subbar-link'
    text='Back to Lineage Object'
    icon='arrow-left'
    click=(route-action 'goBack')
  }}
/>
<ToElsewhere
  @named='md-scroll-spy-record-edit'
  @send={{component
    'control/md-scroll-spy'
    scrollInit=this.scrollTo
    setScrollTo=(route-action 'setScrollTo')
  }}
/>