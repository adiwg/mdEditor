<h4 class="section-header">Editing Step #{{this.stepId}} Citation
  <Control::MdStatus @model={{compute (route-action "parentModel")}} />
</h4>
{{#with this.model as |citation|}}
    <div class="">
      <Object::MdCitation @model={{citation}} @profilePath="record.lineage.processStep.reference" @simpleIdentifier={{true}} />
      {{outlet}}
    </div>
{{/with}}
<ToElsewhere @named="md-subbar-extra" @send={{component "control/subbar-link" text=(concat "Back to Step #" this.stepId) icon="arrow-left" click=(route-action "goBack")}} />
<ToElsewhere @named="md-scroll-spy-record-edit" @send={{component "control/md-scroll-spy"  scrollInit=this.scrollTo setScrollTo=(route-action "setScrollTo")}} />
