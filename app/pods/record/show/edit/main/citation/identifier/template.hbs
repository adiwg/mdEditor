<h4 class="col-xxl-11 col-xxxl-10 col-xxl-offset-1 section-header">
  Editing Identifier: {{model.identifier}}
  <Control::MdStatus @model={{parentModel}} />
</h4>

<div class="row">
  <Object::MdIdentifier @class="col-xxl-11 col-xxxl-10 col-xxl-offset-1" @model={{model}} @profilePath="record.main.citation.identifier" @data-spy="Identifier" />
  {{outlet}}
</div>

<ToElsewhere @named="md-subbar-extra" @send={{component "control/subbar-link" text="Back to Citation" icon="arrow-left" route="record.show.edit.main.citation"}} />
<ToElsewhere @named="md-scroll-spy-record-edit" @send={{component "control/md-scroll-spy"  scrollInit=scrollTo setScrollTo=(route-action "setScrollTo")}} />
