<h4 class='section-header'>
  Editing Entities
  <span class='small'>for <em>{{this.model.title}}</em></span>
  {{control/md-status model=this.model}}
</h4>

{{object/md-objectroute-table
  items=this.model.json.dataDictionary.entity
  header='Entity'
  shadow=true
  buttonText='Add Entity'
  ellipsis=true
  attributes='entityId,codeName,definition'
  editItem=(route-action 'editEntity')
  verticalButtons=true
  profilePath='dictionary.entity'
  hideIndex=false
  condensed=false
}}

{{to-elsewhere
  named='md-subbar-extra'
  send=(component
    'control/subbar-link'
    text='Import CSV'
    icon='sign-in'
    btnType='success'
    route='dictionary.show.edit.entity.import'
  )
}}