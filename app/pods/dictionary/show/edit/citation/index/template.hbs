<h4 class='section-header'>Editing Citation
  <span class='small'>for <em>{{this.model.title}}</em></span>
  {{control/md-status model=this.model}}
</h4>
{{#with this.model.json.dataDictionary.citation as |citation|}}
  {{object/md-citation
    class=''
    model=citation
    profilePath='dictionary.citation'
    editIdentifier=(route-action 'editIdentifier')
  }}
  {{outlet}}
{{/with}}

{{to-elsewhere
  named='md-scroll-spy-dict-edit'
  send=(component
    'control/md-scroll-spy'
    scrollInit=this.scrollTo
    setScrollTo=(route-action 'setScrollTo')
  )
}}