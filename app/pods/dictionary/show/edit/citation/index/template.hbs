<h4 class="section-header">Editing Citation
  <span class="small">for <em>{{model.title}}</em></span>
  <Control::MdStatus @model={{model}} />
</h4>
{{#let model.json.dataDictionary.citation as |citation|}}
  <Object::MdCitation @class="" @model={{citation}} @profilePath="dictionary.citation" @editIdentifier={{route-action "editIdentifier"}} />
  {{outlet}}
{{/let}}

<ToElsewhere @named="md-scroll-spy-dict-edit" @send={{component "control/md-scroll-spy" scrollInit=scrollTo setScrollTo=(route-action "setScrollTo")}} />
