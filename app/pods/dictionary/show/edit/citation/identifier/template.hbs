<h4 class='section-header'>
  Editing Identifier:
  {{this.model.identifier}}
  {{control/md-status model=this.parentModel}}
</h4>

<div class=''>
  {{object/md-identifier
    class=''
    model=this.model
    profilePath='dictionary.citation.identifier'
    data-spy='Identifier'
  }}
  {{outlet}}
</div>

{{to-elsewhere
  named='md-subbar-extra'
  send=(component
    'control/subbar-link'
    text='Back to Citation'
    icon='arrow-left'
    route='dictionary.show.edit.citation'
  )
}}
{{to-elsewhere
  named='md-scroll-spy-dict-edit'
  send=(component
    'control/md-scroll-spy'
    scrollInit=this.scrollTo
    setScrollTo=(route-action 'setScrollTo')
  )
}}