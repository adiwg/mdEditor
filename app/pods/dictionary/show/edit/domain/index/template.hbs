<h4 class='section-header'>
  Editing Domains
  <span class='small'>for <em>{{this.model.title}}</em></span>
  <Control::MdStatus @model={{this.model}} />
</h4>

<Object::MdObjectrouteTable
  @items={{this.model.json.dataDictionary.domain}}
  @header='Domain'
  @shadow={{true}}
  @buttonText='Add Domain'
  @ellipsis={{true}}
  @attributes='domainId,codeName,description'
  @editItem={{route-action 'editDomain'}}
  @verticalButtons={{true}}
  @profilePath='dictionary.domain'
  @hideIndex={{false}}
  @condensed={{false}}
/>