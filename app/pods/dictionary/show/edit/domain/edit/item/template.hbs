<h4 class="section-header">
  Editing Domain Item {{#if model.name}}
    <em>{{model.name}}</em>
  {{else}}
    #{{itemId}}
  {{/if}}
  {{!-- <span class="small">for <em>{{parentModel.codeName}}</em></span> --}}
  <Control::MdStatus @model={{parentModel}} />
</h4>

<Object::MdDomainitem @class="" @model={{model}} @profilePath="dictionary.domain.domainItem" />

<ToElsewhere @named="md-subbar-extra" @send={{component "control/subbar-link"
    text="Back to Domain"
    icon="arrow-left"
    type="primary"
    click=(route-action "backToDomain")
  }} />

<ToElsewhere @named="md-scroll-spy-dict-edit" @send={{component "control/md-scroll-spy" scrollInit=scrollTo setScrollTo=(route-action "setScrollTo")}} />
