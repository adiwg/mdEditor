{
  "name": "mdeditor",
  "version": "1.0.0-dev.1",
  "private": true,
  "description": "A web application for authoring and editing metadata.",
  "repository": {
    "type": "git",
    "url": "https://github.com/adiwg/mdEditor.git"
  },
  "license": "GPLv3",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "doc:toc": "doctoc README.md",
    "build:development": "ember build",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:sass": "stylelint 'app/styles/**/*.scss'",
    "lint:sass:fix": "stylelint 'app/styles/**/*.scss' --fix",
    "format": "npm-run-all format:*",
    "format:hbs": "prettier --write 'app/**/*.hbs'",
    "format:js": "prettier --write '{app,config,stories,tests}/**/*.js' *.js",
    "format:sass": "prettier --write app/**/*.scss",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember try:each --skip-cleanup=true"
  },
  "homepage": "https://github.com/adiwg/mdEditor",
  "bugs": {
    "url": "https://github.com/adiwg/mdEditor/issues"
  },
